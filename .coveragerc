[run]
source = App
branch = True
omit = 
    App/legacy_files/*
    App/__init__.py
    venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    def main()
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
